<html>
<head>
    <script src="http://ajax.aspnetcdn.com/ajax/jQuery/jquery-2.2.0.min.js"></script>
    <script src="http://ajax.aspnetcdn.com/ajax/signalr/jquery.signalr-2.2.0.min.js"></script>
    <script src="/signalr/hubs"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.5.7/angular.min.js"></script>
    <link href="https://fonts.googleapis.com/css?family=Open+Sans|Open+Sans+Condensed:300|Ubuntu" rel="stylesheet">
    <link href="/Static/Styles/main.css" rel="stylesheet" />
    <script src="/Static/Scripts/app.js"></script>
    <script src="/Static/Scripts/LocalFiles/Services/LocalService.js"></script>
    <script src="/Static/Scripts/LocalFiles/Controllers/LocalController.js"></script>
    <script src="/Static/Scripts/LocalFiles/Directives/LocalFileDirective.js"></script>
</head>
<body ng-app="FTP">
    <script type="text/ng-template" id="tree_item_renderer.html">
        <span ng-click="GetFolder(data)">{{data.name}}</span>
        <ul>
            <li ng-repeat="data in data.entries"  ng-include="'tree_item_renderer.html'"></li>
        </ul>
    </script>
    <h2>Filesystem</h2>
    <ul ng-controller="LocalController">
        <li ng-repeat="data in entryPoint.entries" ng-include="'tree_item_renderer.html'"></li>
    </ul>
</body>
</html>